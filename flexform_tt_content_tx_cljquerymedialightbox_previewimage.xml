<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <ROOT>
        <type>array</type>
        <el>
            <previewimageFile>
                <TCEforms>
                    <label>LLL:EXT:cl_jquery_medialightbox/locallang_db.xml:cl_jquery_medialightbox.pi_flexform.previewimagefile</label>
                    <config>
                        <type>group</type>
                        <internal_type>file</internal_type>
                        <allowed>jpg,gif,png</allowed>
                        <uploadfolder>uploads/tx_cljquerymedialightbox</uploadfolder>
                        <maxitems>1</maxitems>
                        <size>1</size>
                        <selectedListStyle>Width:180px</selectedListStyle>
                    </config>
                </TCEforms>
            </previewimageFile>
            <previewimageMaxWidth>
                <TCEforms>
                    <label>LLL:EXT:cl_jquery_medialightbox/locallang_db.xml:tt_content.tx_cljquerymedialightbox_previewimagemaxwidth</label>
                    <config>
                        <type>input</type>
                        <eval>num</eval>
                        <checkbox/>
                        <size>5</size>
                    </config>
                </TCEforms>
            </previewimageMaxWidth>
            <previewimageMaxHeight>
                <TCEforms>
                    <label>LLL:EXT:cl_jquery_medialightbox/locallang_db.xml:tt_content.tx_cljquerymedialightbox_previewimagemaxheight</label>
                    <config>
                        <type>input</type>
                        <eval>num</eval>
                        <checkbox/>
                        <size>5</size>
                    </config>
                </TCEforms>
            </previewimageMaxHeight>
        </el>
    </ROOT>
</T3DataStructure>